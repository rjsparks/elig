  <front>
    <title abbrev="Additional Eligibility Criteria">Additional Criteria for Nominating Committee Eligibility</title>
    <seriesInfo name="Internet-Draft" value="draft-carpenter-eligibility-expand-06"/>
    <author initials="B.E." surname="Carpenter" fullname="Brian E. Carpenter">
      <organization abbrev="Univ. of Auckland">The University of Auckland</organization>
      <address>
        <postal>
          <street>School of Computer Science</street>
